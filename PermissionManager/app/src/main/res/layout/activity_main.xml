<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">




    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Linux ARM64 完美隐藏ROOT演示"
        android:textSize="20dp" />

    <TextView
        android:layout_marginTop="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="新一代root，跟面具完全不同思路，摆脱面具被检测的弱点，完美隐藏root功能（免root级别），兼容安卓APP直接JNI稳定调用"
        />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#ff00ff"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        >

        <ScrollView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:fadeScrollbars="false">

            <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="菜单功能列表："
                />


            <Button
                android:id="@+id/show_myself_info_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:text="1.显示自身权限信息"
                />

            <Button
                android:layout_marginTop="5dp"
                android:id="@+id/get_root_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2.获取ROOT权限"
                />

            <Button
                android:layout_marginTop="5dp"
                android:id="@+id/disable_selinux_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:text="3.绕过SELinux"
                />
            <Button
                android:layout_marginTop="5dp"
                android:id="@+id/enable_selinux_btn"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="4.恢复SELinux"
                />
            <Button
                android:layout_marginTop="5dp"
                android:id="@+id/run_normal_cmd_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="5.执行普通命令"
                />

            <Button
                android:layout_marginTop="5dp"
                android:id="@+id/run_root_cmd_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="6.执行ROOT命令"
                />

            <Button
                android:layout_marginTop="5dp"
                android:id="@+id/adb_root_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="7.授予ADB最高级别权限"
                />

            <Button
                android:layout_marginTop="5dp"
                android:id="@+id/su_env_inject_btn1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="8.授权ROOT到其他APP-[1]"
                />

            <Button
                android:layout_marginTop="5dp"
                android:id="@+id/su_env_inject_btn2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="8.授权ROOT到其他APP-[2]"
                />

            <Button
                android:layout_marginTop="5dp"
                android:id="@+id/clean_su_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="9.清理授权ROOT痕迹"
                />

        </LinearLayout>

        </ScrollView>
        <View
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="#ff00ff"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >

            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="输出信息:"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"

                >
                <Button
                    android:id="@+id/copy_info_btn"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="复制"
                    />
                <Button
                    android:id="@+id/clean_info_btn"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="清空"
                    />
            </LinearLayout>


            <EditText
                android:id="@+id/console_edit"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text=""
                android:inputType="textMultiLine"
                android:gravity="start|top"
                android:textIsSelectable="true"
                android:scrollbars="vertical"
                android:textSize="13dp"
                />

        </LinearLayout>
    </LinearLayout>



</LinearLayout>